<Page x:Class="Progevt43.PageAddRaw"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Progevt43"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageAddRaw">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="350"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>
        <DataGrid x:Name="DGridInfoTabelRaw" AutoGenerateColumns="False" IsReadOnly="True" Grid.Row="1">
            <DataGrid>
                <DataGridTextColumn Header="" Binding="{Binding }" Width="200" ></DataGridTextColumn>
            </DataGrid>
        </DataGrid>
    </Grid>
</Page>
